<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@drawable/shape_feedback_vertical_dialog_bg" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView android:layout_gravity="right" android:id="@+id/ivClose" android:padding="15px" android:layout_width="65px" android:layout_height="65px" android:src="@mipmap/close"/>
    <com.zhy.autolayout.AutoLinearLayout android:orientation="horizontal" android:paddingLeft="23px" android:paddingRight="23px" android:layout_width="match_parent" android:layout_height="80px">
        <com.zhy.autolayout.AutoFrameLayout android:id="@+id/flFeedback" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
            <TextView android:textSize="28px" android:textStyle="bold" android:textColor="@color/selector_color_white_green" android:gravity="center" android:layout_gravity="center_vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/feedback"/>
            <View android:layout_gravity="bottom" android:background="@drawable/selector_transparent_green" android:layout_width="match_parent" android:layout_height="6px" android:layout_marginLeft="120px" android:layout_marginRight="120px"/>
        </com.zhy.autolayout.AutoFrameLayout>
        <View android:background="@color/color_424242" android:layout_width="1px" android:layout_height="match_parent" android:layout_marginTop="10px" android:layout_marginBottom="10px"/>
        <com.zhy.autolayout.AutoFrameLayout android:id="@+id/flService" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1">
            <TextView android:textSize="28px" android:textStyle="bold" android:textColor="@color/selector_color_white_green" android:gravity="center" android:layout_gravity="center_vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/customer_service_land"/>
            <View android:layout_gravity="bottom" android:background="@drawable/selector_transparent_green" android:layout_width="match_parent" android:layout_height="6px" android:layout_marginLeft="120px" android:layout_marginRight="120px"/>
        </com.zhy.autolayout.AutoFrameLayout>
    </com.zhy.autolayout.AutoLinearLayout>
    <include layout="@layout/item_cast_dialog_feedback"/>
    <include layout="@layout/item_feedback_dialog_service"/>
</com.zhy.autolayout.AutoLinearLayout>
