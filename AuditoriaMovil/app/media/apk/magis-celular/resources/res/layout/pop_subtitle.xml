<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/layout_main" android:background="@drawable/bg_pop_radius" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:textSize="16sp" android:textColor="@color/color_333333" android:gravity="center" android:id="@+id/mTextWindowTitle" android:layout_width="match_parent" android:layout_height="88px" android:text="@string/subtitle"/>
    <ImageView android:id="@+id/image_close" android:layout_width="88px" android:layout_height="88px" android:layout_marginRight="10px" android:src="@drawable/playdetail_popup_close" android:scaleType="centerCrop" android:layout_alignParentRight="true"/>
    <ListView android:id="@+id/mListViewSub" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="wrap_content" android:divider="@null" android:layout_below="@+id/image_close" android:overScrollMode="never"/>
</com.zhy.autolayout.AutoRelativeLayout>
