<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dp"/>
            <solid android:color="@color/color_66ffffff"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2dp"/>
                <solid android:color="@color/color_cccccc"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dp"/>
                <gradient android:startColor="@color/color_19d9ff" android:endColor="@color/color_important" android:angle="0" android:centerColor="@color/color_important"/>
            </shape>
        </clip>
    </item>
</layer-list>
